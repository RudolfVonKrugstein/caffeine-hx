<skin name="Tan" assetSwf="hxwidgets_tan.swf">
	<button minWidth="64" minHeight="16" maxWidth="300" maxHeight="120">
		<normal id="button_normal_tan" type="bitmap">
			<scale9Grid x="5" y="5" width="118" height="22" />
		</normal>
		<over id="button_over_tan" type="bitmap">
			<scale9Grid x="5" y="5" width="118" height="22" />
		</over>
		<press id="button_press_tan" type="bitmap">
			<scale9Grid x="5" y="5" width="118" height="22" />
		</press>
	</button>

	<checkbox minWidth="12" minHeight="12" maxWidth="12" maxHeight="12">
		<normal id="checkbox_normal_tan" type="bitmap" />
		<checked id="checkbox_checked_tan" type="bitmap" />
	</checkbox>

	<radio minWidth="12" minHeight="12" maxWidth="12" maxHeight="12">
		<normal id="radio_normal_tan" type="bitmap" />
		<checked id="radio_checked_tan" type="bitmap" />
	</radio>

	<slider minWidth="25" minHeight="15" maxWidth="1024" maxHeight="15">
		<normal>
			<north id="slider_north_tan" type="bitmap" />
			<south id="slider_south_tan" type="bitmap" />
			<west id="slider_west_tan" type="bitmap" />
			<east id="slider_east_tan" type="bitmap" />
		</normal>
		<over>
			<north id="slider_north_over_tan" type="bitmap" />
			<south id="slider_south_over_tan" type="bitmap" />
			<west id="slider_west_over_tan" type="bitmap" />
			<east id="slider_east_over_tan" type="bitmap" />
		</over>
		<colors>
			<color name="border" type="solid" rgb="0x787870" alpha="1.0" />
			<color name="fill" type="solid" rgb="0x000000" alpha="1.0" />
		</colors>
	</slider>
	<progressbar>
			<as3 classname="ProgressBarTan" />
	</progressbar>
</skin>